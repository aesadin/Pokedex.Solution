<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pokedex</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css'>
    <link href="https://fonts.googleapis.com/css2?family=Slackey&family=Yeseva+One&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/styles.css">
  </head>
  <body>
  <style type="text/css">* {cursor: url(https://cur.cursors-4u.net/games/gam-13/gam1282.cur), auto !important;}</style><a href="http://www.cursors-4u.com/cursor/2011/03/31/pokeball-pokemon.html" target="_blank" title="Pokeball - Pokemon"><img src="http://cur.cursors-4u.net/cursor.png" border="0" alt="Pokeball - Pokemon" style="position:absolute; top: 0px; right: 0px;" /></a>
    <div class="nav">
      <nav class="navbar navbar-expand-lg navbar-light">
      <a class="navbar-brand" href="/">Pokedex</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="/Pokemon">Pokemon</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/PokeTypes">Types</a>
          </li>
        </ul>
      </div>
    </div>
    <div>
      <div class="row">
        <div class="col-md-3">
        </div>
        <div class="col-md-6">
          <div class="container">
            <div class="card">
              <div class="card-body scroll">
                @RenderBody()
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-3">
        </div>
      </div>
    </div>
    <div id="footer">
      @Html.Partial("Footer")
    </div>
  </body>
</html>